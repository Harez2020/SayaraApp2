{
  "cleanUrls": true,
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    },
    {
      "source": "/(img|CSS|JS)/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(sw.js|manifest.json)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/delivery",
      "destination": "/index.html"
    },
    {
      "source": "/DeliveryServices",
      "destination": "/index.html"
    },
    {
      "source": "/Delivery/(.*)",
      "destination": "/$1"
    },
    {
      "source": "/Delivery/img/(.*)",
      "destination": "/img/$1"
    },
    {
      "source": "/img/(.*)",
      "destination": "/Delivery/img/$1"
    }
  ]
}