{
  "cleanUrls": true,
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    },
    {
      "source": "/(sw.js|manifest.json)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/Delivery/index.html"
    },
    {
      "source": "/DeliveryServices",
      "destination": "/Delivery/index.html"
    },
    {
      "source": "/(style.css|mobile.css|mobile_utf8.css|services.js|slider.js|translations.js|Navigation.js|PWA.js|sw.js|services.json|manifest.json)",
      "destination": "/Delivery/$1"
    },
    {
      "source": "/img/(.*)",
      "destination": "/Delivery/img/$1"
    }
  ]
}